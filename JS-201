    let d = prompt("Unesi četvrti broj");
    let maks = 0;
    let pom = a;
if(a>b){
if(a>c)
{
if(a>d){
    pom = a;
    maks = pom;
}
else{
    pom = d;
    maks = pom
}}
else{
if(c>d){
    pom = c;
    maks = pom;
}
else{
    pom = d;
    maks = pom;
}}}
else{
if(b>c){
if(b>d){
    pom = b;
    maks = pom;
}
else{
   pom = d;
   maks = pom;
}}
else{
if(c>d){
   pom = c;
   maks = c;
}
else{
  pom = d;
  maks = pom;
}}}
console.log("Najveći između njih je: " + maks);

